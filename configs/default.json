{
  "headers": [
    "id",
    "name",
    "email",
    "phone",
    "status",
    "code"
  ],
  "patterns": {
    "id": "\\d{1,6}",
    "name": "[A-Z][a-z]{1,9} [A-Z][a-z]{1,9}",
    "email": "[a-z]{5,10}\\.[a-z]{5,10}@example\\.com",
    "phone": "\\d{3}-\\d{3}-\\d{4}",
    "status": "(active|inactive|pending)",
    "code": "(\\d{3}|[A-Z]{2}-\\d{2}|TEST\\d{2})"
  }
}